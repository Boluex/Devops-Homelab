$TTL    604800
@       IN      SOA     ns1.homelab. admin.homelab. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL

@       IN      NS      ns1.homelab.
ns1     IN      A       127.0.0.1
django  IN      A       127.0.0.1   ; IP of django container
